function [x] = solve_x(L,b,n)
    y = zeros(n,1);
    y(1) = b(1);
    for i = 2:n
        d = 0;
        for j = 1:i-1
            d = d + L(i,j)*y(j);
        end
        y(i) = b(i) - d;
    end
    
    x = zeros(n,1);
    x(n) = y(n);
    for i = n-1:1
        d = 0;
        for j = (i+1):n
            d = d + L(i,j)*y(j);
        end
        y(i) = b(i) - d;
    end
end